command /metiers [<string>] [<string>]:
	aliases: /metier
	usage: &8/metiers [info | bossbar | actionbar | top]
	trigger:

		if arg 1 is not set:
			execute console command "lp user %player% settemp jobs.command.browse true 5s"
			wait 1 tick
			make player execute command "jobs browse"
			wait 1 tick
			while display name of current inventory of player contains "Menu des Métiers":
				execute console command "lp user %player% settemp jobs.command.join true 10s"
				wait 10 seconds

		else if arg 1 is "info":
			execute console command "lp user %player% settemp jobs.command.stats true 5s"
			wait 1 tick
			make player execute command "jobs stats"

		else if arg 1 is "quete" or "quetes":
			execute console command "lp user %player% settemp jobs.command.quests true 5s"
			wait 1 tick
			make player execute command "jobs quests"

		else if arg 1 is "bossbar":
			execute console command "lp user %player% settemp jobs.command.toggle true 5s"
			wait 1 tick
			make player execute command "jobs toggle bossbar"
		else if arg 1 is "actionbar":
			execute console command "lp user %player% settemp jobs.command.toggle true 5s"
			wait 1 tick
			make player execute command "jobs toggle actionbar"

		else if arg 1 is "top":
			if arg 2 is set:
				execute console command "lp user %player% settemp jobs.command.top true 5s"
				wait 1 tick
				make player execute command "jobs top %arg 2%"
			else:
				message "&7Utilisez /metiers top [Artisan | Bûcheron | Chasseur | Constructeur | Excavateur | Fermier | Forgeron | Mineur | Pêcheur | Éleveur]"
		else if arg 1 is "gtop":
			execute console command "lp user %player% settemp jobs.command.gtop true 5s"
			wait 1 tick
			make player execute command "jobs gtop"
		
		else if arg 1 is "bonus":
			if arg 2 is set:
				execute console command "lp user %player% settemp jobs.command.bonus true 5s"
				wait 1 tick
				make player execute command "jobs bonus %arg 2%"
			else:
				message "&7Utilisez /metiers bonus [Artisan | Bûcheron | Chasseur | Constructeur | Excavateur | Fermier | Forgeron | Mineur | Pêcheur | Éleveur]"

		else:
			message "&7Utilisez /metiers [info | quetes | bonus | bossbar | actionbar | top | gtop]"

command /banque [<string>]:
	usage: &8/banque [ info | interets | top]
	trigger:
		if arg 1 is "info":
			make player execute command "bank info"
		else if arg 1 is "interet" or "interets":
			make player execute command "bank time"
		else if arg 1 is "top" or "classement":
			make player execute command "bank top"
		else:
			message "&7Utilisez /banque [ info | interets | top]"

command /argent [<string>]:
	aliases: /poche
	usage: &8/argent
	trigger:
		if arg 1 is not set:
			make player execute command "balance"
		else if arg 1 is "info" or "poche":
			make player execute command "balance"
		else if arg 1 is "top" or "classement":
			make player execute command "balancetop"
		else:
			message "&7Utilisez &8/argent [info|top]"

command /echange [<string>]:
	usage: &8/echange [joueur]
	trigger:
		if arg 1 is "acc" or "accept" or "accepter":
			make player execute command "trade accept"
		else if arg 1 is "ref" or "refus" or "refuser":
			make player execute command "trade deny"
		else if arg 1 is "info":
			message "&7Utilisez &8/echange [nom de joueur | accepter | refuser | info]"
		else:
			set {_partner} to arg 1 parsed as offline player
			if {_partner} has played before:
				make player execute command "trade %arg 1%"
			else:
				message "&7Il n'y a personne de ce nom sur Gaïa."

command /chat [<string>] [<string>]:
	usage: &8/chat [ info | liste | entrer | quitter | ignorer ] [ channel | nom du joueur à ignorer]
	trigger:
		if arg 1 is "info":
			make player execute command "ch"
		else if arg 1 is "list" or "liste":
			if arg 2 is set:
				make player execute command "ch list %arg 2%"
			else:
				message "&7Vous devez définir un nom de channel."
		else if arg 1 is "entrer":
			if arg 2 is set:
				make player execute command "ch join %arg 2%"
			else:
				message "&7Vous devez définir un nom de channel."
		else if arg 1 is "quitter":
			if arg 2 is set:
				make player execute command "ch leave %arg 2%"
			else:
				message "&7Vous devez définir un nom de channel."
		else if arg 1 is "ignorer" or "ignore":
			if arg 2 is set:
				make player execute command "ignore %arg 2%"
			else:
				message "&7Vous devez définir un nom de joueur."
		else if arg 1 is "messages":
			make player execute command "togglepm"
		else:
			message "&7Utilisez /chat [ info | liste | entrer | quitter | ignorer ] [ channel | nom du joueur à ignorer]"

command /carte [<string>]:
	usage: &8/carte [gaia|factions|iles]
	trigger:
		if arg 1 is not set:
			TellRaw(player, "&7Lien vers la Carte de Gaïa.", "&7&oCliquez ici pour\n&7&oafficher le lien.", "http://carte.gaia-craft.fr/")
		else if arg 1 is "gaia":
			TellRaw(player, "&7Lien vers la Carte de Gaïa.", "&7&oCliquez ici pour\n&7&oafficher le lien.", "http://carte.gaia-craft.fr/")
		else if arg 1 is "factions":
			TellRaw(player, "&7Lien vers la Carte des Factions de Gaïa.", "&7&oCliquez ici pour\n&7&oafficher le lien.", "http://factions.gaia-craft.fr/")
		else if arg 1 is "iles":
			TellRaw(player, "&7Lien vers la Carte des Îles de Gaïa.", "&7&oCliquez ici pour\n&7&oafficher le lien.", "http://iles.gaia-craft.fr/")
		else:
			message "&7Utilisez &8/carte [gaia | faction | iles]"

command /succes [<string>]:
	aliases: /success
	usage: &8/succes [info | stats | livre | top | semaine | mois | notifications]
	trigger:
		if arg 1 is not set:
			execute console command "lp user %player% settemp achievement.list true 5s"
			wait 1 tick
			make player execute command "aach list"
		else if arg 1 is "info":
			execute console command "lp user %player% settemp achievement.list true 5s"
			wait 1 tick
			make player execute command "aach list"
		else if arg 1 is "stats":
			execute console command "lp user %player% settemp achievement.stats true 5s"
			wait 1 tick
			make player execute command "aach stats"
		else if arg 1 is "livre" or "book":
			execute console command "lp user %player% settemp achievement.book true 5s"
			wait 1 tick
			make player execute command "aach book"
		else if arg 1 is "top":
			execute console command "lp user %player% settemp achievement.top true 5s"
			wait 1 tick
			make player execute command "aach top"
		else if arg 1 is "semaine":
			execute console command "lp user %player% settemp achievement.week true 5s"
			wait 1 tick
			make player execute command "aach week"
		else if arg 1 is "mois":
			execute console command "lp user %player% settemp achievement.month true 5s"
			wait 1 tick
			make player execute command "aach month"
		else if arg 1 is "notifications" or "notification":
			execute console command "lp user %player% settemp achievement.toggle true 5s"
			wait 1 tick
			make player execute command "aach toggle"

command /administrateur <player>:
	executable by: console
	trigger:
		if arg 1 has permission "admin.admin":
			wait 4 ticks
			set {_player} to arg 1
			create a gui with virtual chest with 3 rows named "&aOptions":
				format gui slot 0, 1, 2, 3, 4, 5, 6, 18, 19, 20, 21, 22, 23, 24, 25 and 26 with black stained glass named " "
				format gui slot 7 to stick named "&aRetour":
					MenuGUI({_player})
					play resource sound pack "click" to {_player} with volume 1
				format gui slot 8 to barrier named "&c&lFermer":
					close {_player}'s inventory
					play resource sound pack "click" to {_player} with volume 1
			open last gui to {_player}
			
			#set {_Money} to {data::bank.total} + {data::poche.total}
			format slot 4 of arg 1 with chest named "&aInformations" with lore "&7Joueurs : ||&7Total argent : %{_Money}% ¤||&7Comptes : %size of offline players%||&7Mémoire : %total ram%MB / %max ram%MB" to be unstealable
			format slot 10 of arg 1 with arg 1's skull named "&aModération" to close then run [make arg 1 execute command "moderation"]
			
		else:
			message "&d&oLe compte %arg 1% n'est pas administrateur et l'occurrence de la commande /administrateur pour ce même joueur est donc suspecte.%nl%&c&oUn rapport a été envoyé pour prévenir l'administration."
			log "Le joueur %arg 1% n'est pas administrateur et l'occurrence de la commande /administrateur pour ce même joueur est donc suspecte." to "errors.log" 

