command /menu:
	aliases: /m, /me, /men
	usage: &8/menu
	trigger:
		if name of current inventory of player is not "&aMenu":
			close inventory of player
		wait 1 tick
		async
		MenuGUI(player)
		
command /teleportations:
	aliases: /teleportation, /teleport, /tele, /t
	usage: &8/teleportation
	trigger:
		if current server is "hub":
			async
			TeleportationGUI(player)
		else:
			message "&7Vous ne pouvez pas vous téléporter ici."

command /serverteleportations <player>:
	executable by: console
	trigger:
		async
