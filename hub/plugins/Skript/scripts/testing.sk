    trigger:
        make player execute command "progression"


command /apart:
    trigger:
        async:
            set {_loc} to event-location
            drawRings particle "redstone", speed 1, RGB 255, 255, 255, center {_loc}, id "tester1", randomRotation true, animated true, radius 1, ringCount 4, ringDensity 1, visibleRange 32
            drawDot count 50, particle "mobspell", XYZ 255, 255, 255, center {_loc}, visibleRange 32, pulseDelay 1, keepFor 10 seconds
            #draw 10 colored redstone particles at player with RGB 255, 255, 255, visibleRange 10, keepFor 60 ticks
            #drawDot count 10, particle "redstone", RGB 255, 255, 255, center player, visibleRange 10, pulseDelay 5, keepFor 100 ticks
            wait 10 seconds
            stopEffect id "tester1"

command /drawDotEx:
	trigger:
		set {_loc} to event-location
		broadcast "&8[&eskDragon&8]&a&l drawDot syntax examples about to begin, step back a few blocks"
		wait 3 second
		broadcast "&8[&eskDragon&8]&a&l-Example 1-  50 'non colored' particles with offset of '1, 1, 1' and a different pulseDelay each"
		wait 2 second
		broadcast "&b&l3"
		wait 1 second
		broadcast "&b&l2"
		wait 1 second
		broadcast "&b&l1"
		wait 1 second
		drawDot count 50, particle "smoke", XYZ 1, 1, 1, center {_loc}, visibleRange 32, keepFor 5 seconds
		wait 5 seconds
		# Offset is both RGB and XYZ, they are both one in the same
		drawDot count 50, particle "flame", speed 1, RGB 1, 1, 1, center {_loc}, visibleRange 32, pulseDelay 1, keepFor 5 seconds
		wait 5 seconds
		drawDot count 50, particle "heart", speed 10, XYZ 1, 1, 1, center {_loc}, visibleRange 32, pulseDelay 3, keepFor 5 seconds
		wait 5 seconds
		drawDot count 50, particle "itemcrack", material diamond, XYZ 1, 1, 1, center {_loc}, visibleRange 32, pulseDelay 5, keepFor 5 seconds
		wait 5 seconds
		broadcast "&8[&eskDragon&8]&a&l-Example 2- 1 'non colored' particle with offset of 0, 0, 0 and a different pulseDelay each"
		wait 2 second
		broadcast "&b&l3"
		wait 1 second
		broadcast "&b&l2"
		wait 1 second
		broadcast "&b&l1"
		wait 1 second
		drawDot count 1, particle "smoke", RGB 0, 0, 0, center {_loc}, visibleRange 32, keepFor 5 seconds
		wait 5 seconds
		drawDot count 1, particle "flame", XYZ 0, 0, 0, center {_loc}, visibleRange 32, pulseDelay 1, keepFor 5 seconds
		wait 5 seconds
		drawDot count 1, particle "heart", XYZ 0, 0, 0, center {_loc}, visibleRange 32, pulseDelay 3, keepFor 5 seconds
		wait 5 seconds
		drawDot count 1, particle "itemcrack", material wooden sword, XYZ 0, 0, 0, center {_loc}, visibleRange 32, pulseDelay 5, keepFor 5 seconds
		wait 5 seconds
		broadcast "&8[&eskDragon&8]&a&l-Example 3- 1 'colored' particle with different offset values"
		wait 2 second
		broadcast "&b&l3"
		wait 1 second
		broadcast "&b&l2"
		wait 1 second
		broadcast "&b&l1"
		wait 1 second
		# These particles(except note) accept any value from 0 - 255 for offset color (notice how XYZ and RGB in the syntax do the same thing)
		# A value of '0, 0, 0' will produce a black color/shade
		drawDot count 50, particle "redstone", RGB 0, 0, 0, center {_loc}, visibleRange 32, keepFor 5 seconds
		wait 5 seconds
		drawDot count 50, particle "mobspell", XYZ 0, 0, 255, center {_loc}, visibleRange 32, pulseDelay 1, keepFor 5 seconds
		wait 5 seconds
		drawDot count 50, particle "mobspellambient", XYZ 0, 255, 0, center {_loc}, visibleRange 32, pulseDelay 3, keepFor 5 seconds
		wait 5 seconds
		# Note particles ONLY accept an 'X' or 'R' offset value from 0 - 24
		drawDot count 50, particle "note", XYZ 12, 0, 0, center {_loc}, visibleRange 32, pulseDelay 5, keepFor 5 seconds
		wait 5 seconds
		broadcast "&8[&eskDragon&8]&a&l-Example 4- 1 'colored' particle with rainbowMode true and extended time"
		wait 2 second
		broadcast "&b&l3"
		wait 1 second
		broadcast "&b&l2"
		wait 1 second
		broadcast "&b&l1"
		wait 1 second
		# Any offset value will be ignored when rainbowMode is set to true
		drawDot count 50, particle "redstone", RGB 0, 0, 0, center {_loc}, rainbowMode true, visibleRange 32, keepFor 8 seconds
		wait 8 seconds
		drawDot count 50, particle "mobspell", XYZ 0, 255, 0, center {_loc}, rainbowMode true, visibleRange 32, pulseDelay 1, keepFor 8 seconds
		wait 8 seconds
		drawDot count 50, particle "mobspellambient", XYZ 0, 0, 255, center {_loc}, rainbowMode true, visibleRange 32, pulseDelay 3, keepFor 8 seconds
		wait 8 seconds
		drawDot count 50, particle "note", XYZ 0, 0, 0, center {_loc}, rainbowMode true, visibleRange 32, pulseDelay 5, keepFor 8 seconds
		wait 8 seconds
		broadcast "&c&lDone!  :)"

