command /statis:
    trigger:
        message "jobs_jobs : %placeholder ""jobs_jobs"" from player%"
        message "jobs_format: %placeholder ""jobs_format"" from player%"
        message "region :%region at location at player%"
        if "%region at location at player%" contains "resource":
            message "it does"
        #
        message "jobs_level_Bûcheron: %placeholder ""jobs_level_Bûcheron"" from player%"
        message "jobs_level_Mineur: %placeholder ""jobs_level_Artisan"" from player%"
        set {_p1} to placeholder "jobs_level_Mineur" from player parsed as integer
        set {_p2} to placeholder "jobs_level_Bûcheron" from player parsed as integer
        if {_p1} >= 1:
            message "should not"
        if {_p2} >= 1:
            message "yello"

        message "heroes_level: %placeholder ""heroes_class"" from player%"
        message "heroes_main_class_name: %placeholder ""heroes_class2"" from player%"
        message "heroes_second_class_name: %placeholder ""heroes_class_level"" from player%"


command /tree <string>:
    trigger:
        if arg 1 is "info":
            message "info"
            loop tree at location at targeted block:
                message "1"
                add 1 to {_count}
                if {_count} < 200:
                    broadcast "%loop-value%"
                else:
                    message "2"
                    stop

        else if arg 1 is "gen":
            broadcast "gen"
            #grow a tall redwood tree above the targeted block

        else if arg 1 is "del":
            #remove tree at the location at targeted block
            loop tree at location at targeted block:
                add type of loop-value to {_tree::*}
                add location at loop-value to {_location::*}
                set block at location at loop-value to air
            wait 2 seconds
            loop {_tree::*}:
                broadcast "%loop-value%"
                set block at location at {_location::%loop-index%} to loop-value
