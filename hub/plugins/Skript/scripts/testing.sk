command /checking:
	trigger:
		set {_type} to type of player's tool
		message "%{_type}%"
		#drop 1 charcoal named "&fRésidus d'acajou" with lore "&7Des copeaux de bois" and "&7d'un acajou de faible qualité." and " " and "&7Rareté : &fCommun" at player's location
		message "You have %number of ore in player's inventory% ores in your inventory."
		loop a random integer between 1 and {@MaxGoldOre} * {@MaxQuartz} times:
			broadcast "%{@MaxGoldOre} * {@MaxQuartz}%"
		set {_init} to random integer between 1 and 100
		set {_luck} to random integer between 1 and 100
		set {_random} to ({_init} / 100 * 50)
		message "%{_init}% & %{_random}% & %{_luck}%"
		if {_luck} <= {_random}:
			ActionBarSend(player, "Hey {@MaxQuartz}")
		loop ceil({_undefined} * {@MaxQuartz}) times:
			message "ceil"
		loop 0 times:
			message "0"
		set {_mineur.level} to placeholder "jobs_level_Mineur" from player parsed as integer
		message "%{_mineur.level}%"


